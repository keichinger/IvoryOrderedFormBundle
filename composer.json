{
    "name": "egeloen/ordered-form-bundle",
    "type": "symfony-bundle",
    "description": "Provides a form ordering support for your Symfony2 project.",
    "keywords": [ "form", "order" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "minimum-stability": "beta",
    "require": {
        "php": "^7.1",
        "symfony/framework-bundle": "^4.0.0-BETA1",
        "egeloen/ordered-form": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^5.0",
        "symfony/phpunit-bridge": "^4.0.0-BETA1"
    },
    "autoload": {
        "psr-4": { "Ivory\\OrderedFormBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
